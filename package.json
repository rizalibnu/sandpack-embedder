{
  "name": "@rizalibnu/sandpack-embedder",
  "version": "1.1.1",
  "description": "A universal HTML-based Sandpack runtime â€” dynamically inject interactive code editors and previews into any HTML document.",
  "keywords": [
    "sandpack",
    "sandpack html",
    "sandpack ghost",
    "html code playground",
    "ghost code playground"
  ],
  "author": "Rizal Ibnu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rizalibnu/sandpack-embedder"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm clean:build && node esbuild.config.js && tsc --emitDeclarationOnly",
    "clean:build": "rm -rf dist",
    "test": "vitest run --environment jsdom",
    "test:watch": "vitest watch --environment jsdom",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "semantic-release": "semantic-release",
    "build-storybook": "storybook build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@codesandbox/sandpack-react": ">=2.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@codesandbox/sandpack-themes": "^2.0.21",
    "@eslint/js": "^9.39.1",
    "@eslint/json": "^0.14.0",
    "@sebbo2002/semantic-release-jsr": "^3.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@storybook/addon-essentials": "^8.3.1",
    "@storybook/html-vite": "10.1.0-alpha.7",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.1.0",
    "prettier": "^3.6.2",
    "semantic-release": "^25.0.2",
    "storybook": "10.1.0-alpha.7",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.46.4",
    "vitest": "^2.0.0"
  },
  "packageManager": "pnpm@10.4.1",
  "release": {
    "branches": [
      "master"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
